operacion = input('Ingresar operacion: ')
N1 = int(input('Ingresar primer valor: '))
N2 = int(input('Ingresar segundo valor: '))
match operacion:
    case 'Suma':
        S = N1 + N2
        print("El resultado de la suma es: ", S)
    case 'Resta':
        R = N1 - N2
        print("El resultado de la resta es: ", R)
    case 'Multiplicacion':
        M = N1 * N2
        print("El resultado de la multiplicacion es: ", M)
    case 'Division':
        if N2 == 0:
            print('ERROR no se puede dividir para cero', N2)
        else:
            D = N1 / N2
            print('El divisor no es cero', N2)
            print ('El resultado de la division es: ', D)
    case _:
        print ('Operacion no existe')





import math

a = int(input('Ingresar primer valor: '))
b = int(input('Ingredar segundo valor: '))
c = int(input('Ingresar tercer valor: '))

ri = (b ** 2) - 4*a*c
denominador = 2*a

if ri < 0 or denominador == 0:
    print ('---------------------------------------------------------------------')
    print ('Error: No se puede continuar)' )
    if denominador == 0:
        print ('El denominador es cero(a no puede ser cero)')
    if ri < 0:
        print ('La raiz es negativa(numero imaginario)')
    print ('---------------------------------------------------------------------')
else:
    rr = math.sqrt(ri)
    x1 = (-b + rr) / denominador
    x2 = (-b - rr) / denominador
    print(f"Validación exitosa. La raíz es {rr} y el denominador es {denominador}")
    print(f"Resultados: x1 = {x1}, x2 = {x2}")






