lista = [10, -5, 3.5, 7, 0, -2.1, 15, 4.8, 20]

try:
    if not isinstance(lista, list):
        raise TypeError("El dato de entrada debe ser una lista.")
    enteros_positivos = [x for x in lista if type(x) == int and x > 0]

    print(f"Lista original: {lista}")
    print(f"Enteros positivos filtrados: {enteros_positivos}")

    if not enteros_positivos:
        raise ValueError("No se encontraron números enteros positivos en la lista.")

except TypeError as e:
    print(f"Error de tipo: {e}")
except ValueError as e:
    print(f"Aviso: {e}")
except Exception as e:
    print(f"Error inesperado: {e}")





